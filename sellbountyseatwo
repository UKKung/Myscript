local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/Kronos"))()

local window = library:Window({
   Title = "Sell Bounty By Yu HUB (Blox fruit)",
   Accent = Color3.fromRGB(69,69,207),
   Logo = 3610245066,
   ToggleKey = Enum.KeyCode.LeftAlt
})

local tab = window:NewTab({
   Logo = 4483345998
})

local tabsection = tab:TabSection({
   Title = "Home"
})

local column = tabsection:AddColumn({
   Title = "Sell Bounty"
})

local section = column:Section({
   Title = "Sell Bounty"
})

section:Toggle({
   Text = "Sell Bounty",
   State = false,
   Callback = function(state)
       _G.SellBounty = state
       end
})

section:Dropdown({
   Text = "Teleportation",
   List = {'Collossium (Default)', 'Plants', "Font of cafe"},
   Callback = function(Value) 
       _G.Teleportation = Value
       print (_G.Teleportation)
   end
})

section:Toggle({
   Text = "Enable PVP",
   State = false,
   Callback = function(state)
       _G.EnablePVPv2 = state
       end
})

section:Toggle({
   Text = "Enable PVP V2 (Will Not work)",
   State = false,
   Callback = function(state)
       _G.Enablepvp = state
       end
})

local column = tabsection:AddColumn({
   Title = "Team"
})

local section = column:Section({
   Title = "Team"
})

section:Button({
   Text = "Pirate",
   Callback = function() 
        local args = {
            [1] = "SetTeam",
            [2] = "Pirates"
        }

        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
})

section:Button({
   Text = "Marine",
   Callback = function() 
        local args = {
            [1] = "SetTeam",
            [2] = "Marines"
        }
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
})

section:TextLabel({
   Text = "When you gonna change team"
})

section:TextLabel({
   Text = "immediately after you change team, you"
})
section:TextLabel({
   Text = "need to wait 2-5 sec before change team"
})
---funtion
spawn(function()
    while wait() do
        if _G.SellBounty then
            local TweenService = game:GetService("TweenService")
            local Tw = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(2.8, Enum.EasingStyle.Linear, Enum.EasingDirection.Out,0,false,0), 
            {CFrame = CFrame.new(-1836.067138671875, 45.82050323486328, 1364.603759765625)}):Play()
            wait(2.8)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1836.067138671875, 45.82050323486328, 1364.603759765625)
           if game.Players.LocalPlayer.Character.Humanoid.Health <= 0 then
           _G.SellBounty = false
           _G.Enablepvp = false
           wait(3) 
           _G.SellBounty = true
           _G.Enablepvp = true
           wait(0.000000000000001)
           _G.Enablepvp = false
        end
        end
    end
end)

spawn(function()
    while wait() do
        if _G.EnablePVPv2 then
          local A_1 = "EnablePvp"
          local Event = game:GetService("ReplicatedStorage").Remotes["CommF_"]
          Event:InvokeServer(A_1)
        end
    end
end)

spawn(function()
    game:GetService("RunService").Heartbeat:Connect(function()
        if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Humanoid") and _G.SellBounty then
            setfflag("HumanoidParallelRemoveNoPhysics", "False")
            setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
            game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)
        end
    end)
end)

spawn(function()
    while wait() do
        if _G.Enablepvp then
        if game.Players.LocalPlayer.Character.Humanoid.Health <= 0 then
          wait(1.5)
          local A_1 = "EnablePvp"
          local Event = game:GetService("ReplicatedStorage").Remotes["CommF_"]
          Event:InvokeServer(A_1)
            end
        end
    end
end)

